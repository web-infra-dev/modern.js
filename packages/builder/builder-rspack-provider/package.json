{
  "name": "@modern-js/builder-rspack-provider",
  "version": "0.1.0",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "only-allow-pnpm",
    "new": "modern new",
    "build": "wireit",
    "dev": "tsc --watch",
    "example": "node __fixture__/execute",
    "test": "vitest run",
    "test:watch": "vitest dev --no-coverage",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@modern-js/builder-shared": "workspace:*",
    "@modern-js/types": "workspace:*",
    "@modern-js/utils": "workspace:*",
    "@rspack/core": "^0.0.4"
  },
  "devDependencies": {
    "@modern-js/module-tools": "1.20.1",
    "@modern-js/plugin-testing": "1.20.1",
    "@modern-js/tsconfig": "1.20.1",
    "@types/jest": "^26.0.9",
    "@types/node": "^14",
    "@types/react": "^17",
    "husky": "^8",
    "lint-staged": "^11.2.6",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4"
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.sass",
    "*.scss"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "wireit": {
    "build": {
      "command": "tsc",
      "files": [
        "src/**/*",
        "tsconfig.json",
        "package.json",
        "scripts/post-compile.js"
      ],
      "output": [
        "dist/**/*",
        "compiled/config-validate/**/*"
      ]
    }
  }
}
