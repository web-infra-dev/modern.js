{
  "name": "@modern-js/builder-plugin-image-compress",
  "description": "Image compress plugin for modern-js/web-builder",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/web-infra-dev/modern.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/modern.js",
    "directory": "packages/builder/plugin-image-compress"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "version": "2.25.0",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "scripts": {
    "prepublishOnly": "only-allow-pnpm",
    "build": "tsc",
    "dev": "tsc --watch --sourceMap",
    "test": "vitest run",
    "test:watch": "vitest dev --no-coverage"
  },
  "exports": {
    ".": {
      "jsnext:source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./loader": {
      "jsnext:source": "./src/loader.ts",
      "types": "./dist/loader.d.ts",
      "default": "./dist/loader.js"
    }
  },
  "typesVersions": {
    "*": {
      "loader": [
        "./dist/loader.d.ts"
      ]
    }
  },
  "dependencies": {
    "@modern-js/utils": "workspace:*",
    "@napi-rs/image": "^1.3.0",
    "svgo": "^3.0.2"
  },
  "devDependencies": {
    "@modern-js/builder-webpack-provider": "workspace:*",
    "@modern-js/e2e": "workspace:*",
    "@scripts/vitest-config": "workspace:*",
    "@types/node": "^14",
    "typescript": "^5",
    "webpack": "^5.88.1"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true,
    "types": "./dist/index.d.ts"
  }
}
