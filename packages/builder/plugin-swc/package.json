{
  "name": "@modern-js/builder-plugin-swc",
  "version": "1.21.2",
  "description": "SWC plugin for builder in Modern.js",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "swc ./src -d dist --watch",
    "build": "swc ./src -d dist && tsc --emitDeclarationOnly",
    "test": "vitest run --segfault-retry=3",
    "test:watch": "vitest dev --no-coverage",
    "test:update": "SNAPSHOT_UPDATE=1 vitest watch",
    "prepublishOnly": "only-allow-pnpm"
  },
  "exports": {
    ".": "./dist/index.js",
    "./loader": {
      "types": "./dist/loader.d.ts",
      "default": "./dist/loader.js"
    },
    "./plugin": {
      "types": "./dist/plugin.d.ts",
      "default": "./dist/plugin.js"
    },
    "./binding": {
      "types": "./dist/binding.d.ts",
      "default": "./dist/binding.js"
    }
  },
  "typesVersions": {
    "*": {
      "loader": [
        "./dist/loader.d.ts"
      ],
      "plugin": [
        "./dist/plugin.d.ts"
      ],
      "binding": [
        "./dist/binding.d.ts"
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@modern-js/builder-webpack-provider": "workspace:*",
    "@modern-js/utils": "workspace:*",
    "@scripts/vitest-config": "workspace:*",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.18",
    "magic-string": "^0.26.2",
    "source-map": "^0.7.4",
    "typescript": "^4",
    "vite": "^3.0.9",
    "vitest": "^0.22.0",
    "webpack": "^5.74.0"
  },
  "dependencies": {
    "@modern-js/builder-shared": "workspace:*",
    "@modern-js/swc-plugins": "0.0.7",
    "@modern-js/utils": "workspace:*",
    "@swc/helpers": "0.4.12",
    "core-js": "^3.25.0"
  }
}
