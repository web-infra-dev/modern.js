{
  "name": "@modern-js/plugin-changeset",
  "description": "A Progressive React Framework for modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/web-infra-dev/modern.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/modern.js",
    "directory": "packages/cli/plugin-changeset"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.68.20",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "exports": {
    ".": {
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/cjs/index.js"
    },
    "./cli": {
      "jsnext:source": "./src/index.ts",
      "default": "./dist/cjs/index.js"
    },
    "./release-note": {
      "jsnext:source": "./src/commands/releaseNote.ts",
      "import": "./dist/esm/commands/releaseNote.js",
      "require": "./dist/cjs/commands/releaseNote.js"
    }
  },
  "typesVersions": {
    "*": {
      "release-note": [
        "./dist/types/commands/releaseNote.d.ts"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm",
    "new": "modern-lib new",
    "dev": "modern-lib build --watch",
    "build": "modern-lib build",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@changesets/cli": "^2.29.5",
    "@changesets/git": "^2.0.0",
    "@changesets/read": "^0.6.5",
    "@modern-js/plugin-i18n": "workspace:*",
    "@modern-js/utils": "workspace:*",
    "@swc/helpers": "^0.5.17",
    "axios": "^1.7.4",
    "resolve-from": "^5.0.0"
  },
  "devDependencies": {
    "@modern-js/core": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@types/jest": "^29",
    "@types/node": "^18",
    "jest": "^29",
    "typescript": "^5"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/index.d.ts"
  }
}
