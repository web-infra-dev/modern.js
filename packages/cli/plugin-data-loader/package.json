{
  "name": "@modern-js/plugin-data-loader",
  "description": "A Progressive React Framework for modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/web-infra-dev/modern.js/issues",
  "repository": "web-infra-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.9.0",
  "engines": {
    "node": ">=14.17.6"
  },
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    "./loader": {
      "jsnext:source": "./src/cli/loader.ts",
      "types": "./dist/types/cli/loader.d.ts",
      "default": "./dist/cjs/cli/loader.js"
    },
    "./server": {
      "jsnext:source": "./src/server/index.ts",
      "types": "./dist/types/server/index.d.ts",
      "default": "./dist/cjs/server/index.js"
    },
    "./runtime": {
      "jsnext:source": "./src/runtime/index.ts",
      "types": "./dist/types/runtime/index.d.ts",
      "default": "./dist/esm/runtime/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "loader": [
        "./dist/types/cli/loader.d.ts"
      ],
      "server": [
        "./dist/types/server/index.d.ts"
      ],
      "runtime": [
        "./dist/types/runtime/index.d.ts"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm",
    "new": "modern-lib new",
    "build": "modern-lib build",
    "dev": "modern-lib build --watch",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@babel/core": "^7.18.0",
    "@babel/runtime": "^7.18.0",
    "@modern-js/utils": "workspace:*",
    "@remix-run/node": "^1.12.0",
    "path-to-regexp": "^6.2.0",
    "react-router-dom": "^6.8.1"
  },
  "devDependencies": {
    "@modern-js/core": "workspace:*",
    "@modern-js/server-core": "workspace:*",
    "@modern-js/types": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@types/babel__core": "^7.1.15",
    "@types/jest": "^29",
    "@types/node": "^14",
    "@types/supertest": "^2.0.11",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29",
    "memfs": "^3.3.0",
    "supertest": "^6.1.6",
    "ts-jest": "^29.0.5",
    "typescript": "^4",
    "webpack": "^5.76.2",
    "webpack-chain": "^6.5.1"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/index.d.ts"
  }
}
