    "@types/babel__core": "^7.20.5",
    "@types/node": "^20",
    "@types/supertest": "^2.0.16",
    "isomorphic-fetch": "^3.0.0",
    "memfs": "^3.5.3",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "supertest": "^6.3.4",
    "typescript": "^5",
    "@rsbuild/core": "1.7.1"
  },
  "peerDependencies": {
    "react": ">=17.0.2"
  },
  "sideEffects": false,
