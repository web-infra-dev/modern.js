{
  "name": "@modern-js/plugin-storybook",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.18.0",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/runtime/index.js",
  "jsnext:modern": "./dist/js/modern/runtime/index.js",
  "exports": {
    ".": {
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    },
    "./cli": {
      "jsnext:source": "./src/index.ts",
      "default": "./dist/js/node/index.js"
    },
    "./addons/runtime-preset": {
      "jsnext:source": "./addons/runtime-preset.js",
      "default": "./addons/runtime-preset.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "only-allow-pnpm && pnpm build --platform",
    "new": "modern new",
    "build": "wireit",
    "test": "wireit",
    "dev": "modern build --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.0",
    "@modern-js/utils": "workspace:*",
    "@modern-js/webpack": "workspace:*",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storysource": "^6.3.8",
    "@storybook/builder-webpack5": "^6.3.8",
    "@storybook/core": "^6.3.8",
    "@storybook/manager-webpack5": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "esbuild": "^0.14.38",
    "findup-sync": "^4.0.0",
    "fs-extra": "^10.0.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "process.argv": "^0.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@modern-js/core": "workspace:*",
    "@modern-js/runtime": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@storybook/addons": "^6.3.7",
    "@types/findup-sync": "^4.0.1",
    "@types/fs-extra": "^9.0.12",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27",
    "@types/node": "^14",
    "jest": "^27",
    "react": "^17",
    "react-dom": "^17",
    "typescript": "^4",
    "webpack": "^5.71.0",
    "webpack-chain": "^6.5.1"
  },
  "peerDependencies": {
    "@modern-js/runtime": "workspace:^1.18.0",
    "react": ">=17"
  },
  "sideEffects": false,
  "modernConfig": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/index.d.ts"
  },
  "wireit": {
    "build": {
      "command": "modern build",
      "files": [
        "src/**/*",
        "tsconfig.json",
        "package.json"
      ],
      "output": [
        "dist/**/*"
      ]
    },
    "test": {
      "command": "jest --passWithNoTests",
      "files": [
        "src/**/*",
        "tsconfig.json",
        "package.json",
        "tests/**/*"
      ],
      "output": []
    }
  }
}
