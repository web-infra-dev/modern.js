{
  "name": "@modern-js/plugin-storybook",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.1.5",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/runtime/index.js",
  "jsnext:modern": "./dist/js/modern/runtime/index.js",
  "exports": {
    ".": {
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    },
    "./cli": {
      "jsnext:source": "./src/index.ts",
      "default": "./dist/js/node/index.js"
    },
    "./addons/runtime-preset": {
      "jsnext:source": "./addons/runtime-preset.js",
      "default": "./addons/runtime-preset.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/esbuild-compiler": "workspace:^0.1.1",
    "@modern-js/plugin-router": "workspace:^1.1.1",
    "@modern-js/plugin-state": "workspace:^1.1.3",
    "@modern-js/utils": "workspace:^1.1.5",
    "@modern-js/webpack": "workspace:^1.1.2",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storysource": "^6.3.8",
    "@storybook/builder-webpack5": "^6.3.8",
    "@storybook/core": "^6.3.8",
    "@storybook/manager-webpack5": "^6.3.8",
    "@storybook/react": "^6.3.8",
    "findup-sync": "^4.0.0",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.7",
    "import-lazy": "^4.0.0",
    "lodash.merge": "^4.6.2",
    "lodash.template": "^4.5.0",
    "node-polyfill-webpack-plugin": "^1.1.4",
    "process.argv": "^0.6.0"
  },
  "devDependencies": {
    "@modern-js/core": "workspace:^1.2.0",
    "@modern-js/module-tools": "^1.1.3",
    "@modern-js/module-tools-hooks": "workspace:^1.1.2",
    "@modern-js/plugin-testing": "^1.2.0",
    "@modern-js/runtime": "workspace:^1.1.1",
    "@modern-js/runtime-core": "workspace:^1.1.2",
    "@storybook/addons": "^6.3.7",
    "@types/findup-sync": "^4.0.1",
    "@types/fs-extra": "^9.0.12",
    "@types/glob": "^7.1.4",
    "@types/jest": "^26",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.template": "^4.5.0",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "commander": "^8.1.0",
    "typescript": "^4",
    "webpack": "^5.54.0",
    "webpack-chain": "^6.5.1"
  },
  "peerDependencies": {
    "@modern-js/core": "workspace:^1.2.0",
    "@modern-js/runtime": "workspace:^1.1.1",
    "@modern-js/runtime-core": "workspace:^1.1.2"
  },
  "sideEffects": false,
  "modernConfig": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
