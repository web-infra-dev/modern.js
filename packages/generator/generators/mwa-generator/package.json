{
  "name": "@modern-js/mwa-generator",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "3.0.0-beta.1",
  "jsnext:source": "./src/index.ts",
  "main": "./src/index.ts",
  "files": [
    "/templates",
    "/dist/index.js"
  ],
  "scripts": {
    "prepublishOnly": "only-allow-pnpm && pnpm build --platform",
    "new": "modern-lib new",
    "build": "modern-lib build",
    "test": "jest --passWithNoTests"
  },
  "devDependencies": {
    "@babel/runtime": "^7.18.0",
    "@modern-js/base-generator": "workspace:*",
    "@modern-js/codesmith": "2.0.0",
    "@modern-js/codesmith-api-app": "2.0.0",
    "@modern-js/codesmith-api-json": "2.0.0",
    "@modern-js/dependence-generator": "workspace:*",
    "@modern-js/entry-generator": "workspace:*",
    "@modern-js/generator-common": "workspace:*",
    "@modern-js/generator-utils": "workspace:*",
    "@modern-js/packages-generator": "workspace:*",
    "@modern-js/plugin-i18n": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@types/jest": "^27",
    "@types/node": "^14",
    "jest": "^27",
    "typescript": "^4"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js",
      "disableSourceMap": true
    }
  },
  "publishConfig": {
    "main": "./dist/index.js",
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "types": "./src/index.ts"
}
