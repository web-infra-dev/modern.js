{
  "name": "@modern-js/plugin-garfish",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.1.0",
  "jsnext:source": "./src/cli/index.ts",
  "types": "./src/cli/index.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/runtime/index.d.ts"
      ],
      "cli": [
        "./dist/types/cli/index.d.ts"
      ],
      "runtime": [
        "./dist/types/runtime/index.d.ts"
      ],
      "deps": [
        "./dist/types/deps/index.d.ts"
      ],
      "types": [
        "./type.d.ts"
      ]
    }
  },
  "main": "./dist/js/node/cli/index.js",
  "module": "./dist/js/treeshaking/runtime/index.js",
  "jsnext:modern": "./dist/js/modern/cli/index.js",
  "exports": {
    "./types": "./type.d.ts",
    ".": {
      "jsnext:source": "./src/cli/index.ts",
      "default": "./dist/js/node/cli/index.js"
    },
    "./cli": {
      "jsnext:source": "./src/cli/index.ts",
      "default": "./dist/js/node/cli/index.js"
    },
    "./deps": {
      "jsnext:source": "./src/deps/index.ts",
      "node": "./dist/js/node/deps/index.js",
      "default": "./dist/js/treeshaking/deps/index.js"
    },
    "./runtime": {
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/runtime/index.js"
    }
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm && pnpm build --platform",
    "new": "modern-lib new",
    "build": "modern-lib build",
    "dev": "modern-lib build --watch",
    "build:watch": "rm -rf ./dist && modern build --watch",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.0",
    "@modern-js/utils": "workspace:*",
    "@types/debug": "^4.1.7",
    "@types/react-loadable": "^5.5.6",
    "debug": "^4.3.2",
    "garfish": "^1.8.1",
    "hoist-non-react-statics": "^3.3.2",
    "react-loadable": "^5.5.0"
  },
  "peerDependencies": {
    "@modern-js/runtime": "workspace:^2.1.0"
  },
  "peerDependenciesMeta": {
    "@modern-js/runtime": {
      "optional": true
    }
  },
  "devDependencies": {
    "@modern-js/app-tools": "workspace:*",
    "@modern-js/core": "workspace:*",
    "@modern-js/runtime": "workspace:*",
    "@modern-js/plugin-router-v5": "workspace:*",
    "@modern-js/types": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^27",
    "@types/node": "^14",
    "@types/testing-library__jest-dom": "^5.14.3",
    "jest": "^27",
    "jest-fetch-mock": "^3.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-router-dom": "^6.6.0",
    "typescript": "^4",
    "webpack-chain": "^6.5.1"
  },
  "sideEffects": false,
  "modernConfig": {},
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/cli/index.d.ts"
  }
}
