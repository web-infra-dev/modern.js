{
  "compilerOptions": {
    "lib": ["ES2021"],
    "module": "ESNext",
    "jsx": "react-jsx",
    "strict": true,
    "skipLibCheck": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "moduleResolution": "bundler",
    "useDefineForClassFields": true,
    "types": ["@rsbuild/core/types"],
    "paths": {
      "@vitest/expect": ["node_modules/@vitest/expect"]
    }
  }
}
