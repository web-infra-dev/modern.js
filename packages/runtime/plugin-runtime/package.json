    "typescript": "^5",
    "webpack": "^5.88.1"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true,
    "types": "./dist/types/index"
  }
