{
  "name": "@modern-js/server-core",
  "description": "A Progressive React Framework for modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/web-infra-dev/modern.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/modern.js",
    "directory": "packages/server/core"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "3.0.0-alpha.0",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm-node/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "modern:source": "./src/index.ts",
      "jsnext:source": "./src/index.ts",
      "node": {
        "import": "./dist/esm-node/index.mjs",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/cjs/index.js"
    },
    "./hono": {
      "types": "./dist/types/hono.d.ts",
      "jsnext:source": "./src/hono.ts",
      "default": "./dist/cjs/hono.js"
    },
    "./node": {
      "types": "./dist/types/adapters/node/index.d.ts",
      "modern:source": "./src/adapters/node/index.ts",
      "jsnext:source": "./src/adapters/node/index.ts",
      "default": "./dist/cjs/adapters/node/index.js"
    },
    "./edge": {
      "types": "./dist/types/adapters/edge/index.d.ts",
      "modern:source": "./src/adapters/edge/index.ts",
      "jsnext:source": "./src/adapters/edge/index.ts",
      "node": {
        "import": "./dist/esm-node/adapters/edge/index.mjs",
        "require": "./dist/cjs/adapters/edge/index.js"
      },
      "default": "./dist/cjs/adapters/edge/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "node": [
        "./dist/types/adapters/node/index.d.ts"
      ],
      "edge": [
        "./dist/types/adapters/edge/index.d.ts"
      ],
      "hono": [
        "./dist/types/hono.d.ts"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm",
    "dev": "rslib build --watch",
    "build": "rslib build",
    "test": "rstest --passWithNoTests"
  },
  "engines": {
    "node": ">=20"
  },
  "dependencies": {
    "@modern-js/plugin": "workspace:*",
    "@modern-js/runtime-utils": "workspace:*",
    "@modern-js/utils": "workspace:*",
    "@swc/helpers": "^0.5.17",
    "@web-std/fetch": "^4.2.1",
    "@web-std/file": "^3.0.3",
    "@web-std/stream": "^1.0.3",
    "cloneable-readable": "^3.0.0",
    "flatted": "^3.3.3",
    "hono": "^4.10.4",
    "ts-deepmerge": "7.0.3"
  },
  "devDependencies": {
    "@modern-js/types": "workspace:*",
    "@scripts/rstest-config": "workspace:*",
    "@rslib/core": "0.18.5",
    "@types/cloneable-readable": "^2.0.3",
    "@types/merge-deep": "^3.0.3",
    "@types/node": "^20",
    "http-proxy-middleware": "^2.0.9",
    "@modern-js/rslib": "workspace:*",
    "typescript": "^5"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/esm-node/index.mjs",
        "default": "./dist/cjs/index.js"
      },
      "./hono": {
        "types": "./dist/types/hono.d.ts",
        "import": "./dist/esm-node/hono.mjs",
        "default": "./dist/cjs/hono.js"
      },
      "./node": {
        "types": "./dist/types/adapters/node/index.d.ts",
        "import": "./dist/esm-node/adapters/node/index.mjs",
        "default": "./dist/cjs/adapters/node/index.js"
      },
      "./edge": {
        "types": "./dist/types/adapters/edge/index.d.ts",
        "import": "./dist/esm-node/adapters/edge/index.mjs",
        "default": "./dist/cjs/adapters/edge/index.js"
      }
    }
  }
}
