{
  "name": "@modern-js/server-core",
  "description": "A Progressive React Framework for modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/web-infra-dev/modern.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/modern.js",
    "directory": "packages/server/core"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.47.0",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/cjs/index.js"
    },
    "./base": {
      "types": "./dist/types/base/index.d.ts",
      "jsnext:source": "./src/base/index.ts",
      "default": "./dist/cjs/base/index.js"
    },
    "./base/browserify": {
      "types": "./dist/types/base/serverBase.d.ts",
      "jsnext:source": "./src/base/serverBase.ts",
      "default": "./dist/esm-node/base/serverBase.js"
    },
    "./middlewares": {
      "types": "./dist/types/base/middlewares/index.d.ts",
      "jsnext:source": "./src/base/middlewares/index.ts",
      "require": "./dist/cjs/base/middlewares/index.js",
      "default": "./dist/esm-node/base/middlewares/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "base": [
        "./dist/types/base/index.d.ts"
      ],
      "base/browserify": [
        "./dist/types/base/serverBase.d.ts"
      ],
      "middlewares": [
        "./dist/types/base/middlewares/index.d.ts"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm",
    "new": "modern-lib new",
    "dev": "modern-lib build --watch",
    "build": "modern-lib build",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@modern-js/plugin": "workspace:*",
    "@modern-js/runtime-utils": "workspace:*",
    "@modern-js/utils": "workspace:*",
    "@swc/helpers": "0.5.3",
    "@web-std/fetch": "^4.2.1",
    "@web-std/stream": "^1.0.3",
    "hono": "^3.12.2",
    "isbot": "3.8.0",
    "merge-deep": "^3.0.3"
  },
  "devDependencies": {
    "@modern-js/types": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@types/jest": "^29",
    "@types/merge-deep": "^3.0.0",
    "@types/node": "^14",
    "http-proxy-middleware": "^2.0.4",
    "jest": "^29",
    "ts-jest": "^29.1.0",
    "typescript": "^5"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true,
    "types": "./dist/types/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/types/index.d.ts",
        "node": {
          "jsnext:source": "./src/index.ts",
          "import": "./dist/esm/index.js",
          "require": "./dist/cjs/index.js"
        },
        "default": "./dist/cjs/index.js"
      },
      "./base": {
        "types": "./dist/types/base/index.d.ts",
        "jsnext:source": "./src/base/index.ts",
        "default": "./dist/cjs/base/index.js"
      },
      "./base/browserify": {
        "types": "./dist/types/base/serverBase.d.ts",
        "jsnext:source": "./src/base/serverBase.ts",
        "default": "./dist/esm-node/base/serverBase.js"
      },
      "./middlewares": {
        "types": "./dist/types/base/middlewares/index.d.ts",
        "jsnext:source": "./src/base/middlewares/index.ts",
        "require": "./dist/cjs/base/middlewares/index.js",
        "default": "./dist/esm-node/base/middlewares/index.js"
      }
    }
  }
}
