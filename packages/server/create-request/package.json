{
  "name": "@modern-js/create-request",
  "description": "A Progressive React Framework for modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/web-infra-dev/modern.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/modern.js",
    "directory": "packages/server/create-request"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.68.1",
  "jsnext:source": "./src/node.ts",
  "types": "./src/browser.ts",
  "main": "./dist/cjs/node.js",
  "module": "./dist/esm/node.mjs",
  "browser": "./dist/esm/browser.mjs",
  "files": [
    "dist",
    "hook.d.ts"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/browser.d.ts",
      "jsnext:source": "./src/browser.ts",
      "node": "./dist/esm-node/node.mjs",
      "default": "./dist/esm/browser.mjs"
    },
    "./client": {
      "types": "./dist/types/browser.d.ts",
      "jsnext:source": "./src/browser.ts",
      "default": "./dist/esm/browser.mjs"
    },
    "./modern": {
      "jsnext:source": "./src/browser.ts",
      "default": "./dist/esm-node/browser.mjs"
    },
    "./server": {
      "types": "./dist/types/node.d.ts",
      "jsnext:source": "./src/node.ts",
      "node": {
        "import": "./dist/esm-node/node.mjs",
        "require": "./dist/cjs/node.js"
      },
      "default": "./dist/cjs/node.js"
    },
    "./browser": {
      "types": "./dist/types/browser.d.ts",
      "jsnext:source": "./src/browser.ts",
      "default": "./dist/esm/browser.mjs"
    },
    "./default": {
      "types": "./dist/types/node.d.ts",
      "jsnext:source": "./src/node.ts",
      "node": {
        "import": "./dist/esm-node/node.mjs",
        "require": "./dist/cjs/node.js"
      },
      "default": "./dist/cjs/node.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/browser.d.ts"
      ],
      "client": [
        "./dist/types/browser.d.ts"
      ],
      "modern": [
        "./dist/types/browser.d.ts"
      ],
      "server": [
        "./dist/types/node.d.ts"
      ],
      "browser": [
        "./dist/types/browser.d.ts"
      ],
      "default": [
        "./dist/types/node.d.ts"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm",
    "new": "modern-lib new",
    "dev": "rslib build --watch",
    "build": "rslib build",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@modern-js/runtime-utils": "workspace:*",
    "@modern-js/utils": "workspace:*",
    "@swc/helpers": "^0.5.17",
    "encoding": "^0.1.13",
    "node-fetch": "^2.7.0",
    "path-to-regexp": "^6.3.0",
    "query-string": "^7.1.3"
  },
  "devDependencies": {
    "@modern-js/types": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@rslib/core": "0.18.0",
    "@modern-js/rslib": "workspace:*",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/node-fetch": "^2.6.13",
    "isomorphic-fetch": "^3.0.0",
    "jest": "^29.7.0",
    "nock": "^13.5.6",
    "typescript": "^5"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/browser.d.ts"
  }
}
