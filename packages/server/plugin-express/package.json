{
  "name": "@modern-js/plugin-express",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.6.0",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    "./types": "./types.d.ts",
    ".": {
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    },
    "./cli": {
      "jsnext:source": "./src/cli/index.ts",
      "default": "./dist/js/node/cli/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "only-allow-pnpm && pnpm build -- --platform",
    "new": "modern new",
    "build": "wireit",
    "test": "wireit"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.0",
    "@modern-js/adapter-helpers": "workspace:^1.3.0",
    "@modern-js/bff-core": "workspace:^1.1.2",
    "@modern-js/bff-runtime": "workspace:^1.3.0",
    "@modern-js/types": "workspace:^1.6.0",
    "@modern-js/utils": "workspace:^1.7.12",
    "cookie-parser": "^1.4.5",
    "finalhandler": "^1.1.2",
    "formidable": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "type-is": "^1.6.18"
  },
  "devDependencies": {
    "@modern-js/core": "workspace:*",
    "@modern-js/server-core": "workspace:*",
    "@modern-js/server-utils": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/finalhandler": "^1.1.1",
    "@types/formidable": "^1.2.3",
    "@types/jest": "^27",
    "@types/node": "^14",
    "@types/supertest": "^2.0.11",
    "@types/type-is": "^1.6.3",
    "express": "^4.17.1",
    "jest": "^27",
    "supertest": "^6.1.6",
    "typescript": "^4",
    "zod": "^3.17.3"
  },
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "peerDependencies": {
    "express": "^4.17.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/index.d.ts"
  },
  "wireit": {
    "build": {
      "command": "modern build",
      "files": [
        "src/**/*",
        "tsconfig.json",
        "package.json"
      ],
      "output": [
        "dist/**/*"
      ]
    },
    "test": {
      "command": "jest --passWithNoTests",
      "files": [
        "src/**/*",
        "tsconfig.json",
        "package.json",
        "tests/**/*"
      ],
      "output": []
    }
  }
}
