{
  "name": "@modern-js/plugin-koa",
  "description": "A Progressive React Framework for modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/web-infra-dev/modern.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/modern.js",
    "directory": "packages/server/plugin-koa"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.70.7",
  "jsnext:source": "./src/index.ts",
  "types": "./src/cli",
  "main": "./dist/cjs/cli/index.js",
  "module": "./dist/esm/cli/index.js",
  "scripts": {
    "prepublishOnly": "only-allow-pnpm",
    "new": "modern-lib new",
    "dev": "modern-lib build --watch",
    "build": "modern-lib build",
    "test": "jest --passWithNoTests"
  },
  "exports": {
    "./types": "./types.d.ts",
    ".": {
      "types": "./dist/types/cli/index.d.ts",
      "jsnext:source": "./src/cli/index.ts",
      "default": "./dist/cjs/cli/index.js"
    },
    "./cli": {
      "types": "./dist/types/cli/index.d.ts",
      "jsnext:source": "./src/cli/index.ts",
      "default": "./dist/cjs/cli/index.js"
    },
    "./runtime": {
      "types": "./dist/types/runtime/index.d.ts",
      "jsnext:source": "./src/runtime.ts",
      "default": "./dist/cjs/runtime.js"
    },
    "./server": {
      "types": "./dist/types/index.d.ts",
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/esm-node/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/cli/"
      ],
      "cli": [
        "./dist/types/cli"
      ],
      "runtime": [
        "./dist/types/runtime.d.ts"
      ],
      "server": [
        "./dist/types/index.d.ts"
      ]
    }
  },
  "dependencies": {
    "@modern-js/bff-core": "workspace:*",
    "@modern-js/bff-runtime": "workspace:*",
    "@modern-js/server-core": "workspace:*",
    "@modern-js/types": "workspace:*",
    "@modern-js/utils": "workspace:*",
    "@swc/helpers": "^0.5.17",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1",
    "type-is": "^1.6.18"
  },
  "devDependencies": {
    "@modern-js/app-tools": "workspace:*",
    "@modern-js/core": "workspace:*",
    "@modern-js/plugin-v2": "workspace:*",
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@types/jest": "^29",
    "@types/koa": "2.15.0",
    "@types/koa-router": "^7.4.4",
    "@types/node": "^18",
    "@types/supertest": "^2.0.11",
    "@types/type-is": "^1.6.3",
    "jest": "^29",
    "koa": "2.16.3",
    "supertest": "^6.1.6",
    "typescript": "^5",
    "zod": "^3.22.3"
  },
  "peerDependencies": {
    "koa": "^2.16.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/cli/index.d.ts"
  }
}
