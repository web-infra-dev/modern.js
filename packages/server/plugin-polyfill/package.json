{
  "name": "@modern-js/plugin-polyfill",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.0.0-beta.4",
  "jsnext:source": "./src/cli.ts",
  "types": "./src/cli.ts",
  "main": "./dist/js/node/cli.js",
  "module": "./dist/js/treeshaking/cli.js",
  "jsnext:modern": "./dist/js/modern/cli.js",
  "exports": {
    ".": {
      "jsnext:source": "./src/cli.ts",
      "node": {
        "import": "./dist/js/node/cli.js",
        "require": "./dist/js/node/cli.js"
      },
      "default": "./dist/js/node/cli.js"
    },
    "./cli": {
      "jsnext:source": "./src/cli.ts",
      "node": {
        "import": "./dist/js/node/cli.js",
        "require": "./dist/js/node/cli.js"
      },
      "default": "./dist/js/node/cli.js"
    },
    "./server": {
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm && pnpm build --platform",
    "new": "modern-lib new",
    "build": "modern-lib build",
    "dev": "modern-lib build --watch",
    "test": "jest --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.0",
    "@modern-js/utils": "workspace:*",
    "@modern-js/polyfill-lib": "^1.0.0",
    "lru-cache": "^6.0.0",
    "ua-parser-js": "^0.7.28"
  },
  "devDependencies": {
    "@modern-js/server-core": "workspace:*",
    "@modern-js/app-tools": "workspace:*",
    "@modern-js/types": "workspace:*",
    "@types/jest": "^27",
    "@types/lru-cache": "^5.1.1",
    "@types/node": "^14",
    "@types/ua-parser-js": "^0.7.36",
    "typescript": "^4",
    "@modern-js/core": "workspace:*",
    "@scripts/build": "workspace:*",
    "jest": "^27",
    "@scripts/jest-config": "workspace:*"
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/cli.d.ts"
  }
}
