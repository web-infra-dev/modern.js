{
  "name": "@modern-js/plugin-server",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.0.0-beta.4",
  "types": "./src/cli",
  "jsnext:source": "./src/cli",
  "main": "./dist/js/node/cli.js",
  "exports": {
    ".": {
      "jsnext:source": "./src/cli.ts",
      "default": "./dist/js/node/cli.js"
    },
    "./cli": {
      "jsnext:source": "./src/cli.ts",
      "default": "./dist/js/node/cli.js"
    },
    "./server": {
      "jsnext:source": "./src/server.ts",
      "default": "./dist/js/node/server.js"
    }
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm && modern build --platform",
    "dev": "modern-lib build --watch",
    "build": "modern-lib build",
    "new": "modern-lib new",
    "test": "jest --passWithNoTests"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "cli": [
        "./dist/types/cli.d.ts"
      ],
      "server": [
        "./dist/types/server.d.ts"
      ]
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.18.0",
    "@modern-js/babel-compiler": "workspace:*",
    "@modern-js/server-utils": "workspace:*",
    "@modern-js/utils": "workspace:*"
  },
  "devDependencies": {
    "@modern-js/server-core": "workspace:*",
    "@modern-js/app-tools": "workspace:*",
    "@modern-js/core": "workspace:*",
    "@scripts/build": "workspace:*",
    "@modern-js/types": "workspace:*",
    "typescript": "^4",
    "@types/jest": "^27",
    "@types/node": "^14",
    "jest": "^27",
    "@scripts/jest-config": "workspace:*"
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.sass",
    "*.scss"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/types/cli.d.ts"
  }
}
