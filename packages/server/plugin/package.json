{
  "name": "@modern-js/server-plugin",
  "version": "1.0.0-rc.2",
  "jsnext:source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "pnpm build -- --platform",
    "new": "modern new",
    "build": "modern build",
    "test": "modern test --passWithNoTests"
  },
  "dependencies": {
    "@babel/runtime": "^7",
    "@modern-js/plugin": "workspace:^1.0.0-rc.2"
  },
  "devDependencies": {
    "@modern-js/core": "workspace:^1.0.0-rc.2",
    "@modern-js/types": "workspace:^1.0.0-rc.2",
    "@types/jest": "^26",
    "@types/node": "^14",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "ts-jest": "^27.0.4",
    "typescript": "^4",
    "@modern-js/plugin-testing": "^1.0.0-rc.2",
    "@modern-js/module-tools": "^1.0.0-rc.2"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  }
}
