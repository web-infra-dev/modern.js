{
  "name": "@modern-js/prod-server",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "2.1.0",
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    },
    "./worker": {
      "node": {
        "jsnext:source": "./src/worker-server.ts",
        "import": "./dist/js/modern/worker-server.js",
        "require": "./dist/js/node/worker-server.js"
      },
      "default": "./dist/js/treeshaking/worker-server.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "worker": [
        "./dist/types/worker-server.d.ts"
      ]
    }
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm && pnpm build --platform",
    "new": "modern-lib new",
    "build": "modern-lib build",
    "dev": "modern-lib build --watch",
    "test": "jest"
  },
  "dependencies": {
    "@modern-js/utils": "workspace:*",
    "@modern-js/server-core": "workspace:*",
    "etag": "^1.8.1",
    "fresh": "^0.5.2",
    "cookie": "^0.4.2",
    "http-proxy-middleware": "^2.0.1",
    "ignore-styles": "^5.0.1",
    "lru-cache": "^6.0.0",
    "merge-deep": "^3.0.3",
    "path-to-regexp": "^6.2.0",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "@modern-js/types": "workspace:*",
    "@modern-js/server-core": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@scripts/build": "workspace:*",
    "@types/cookie": "^0.4.1",
    "@types/jest": "^27",
    "@types/lru-cache": "^5.1.1",
    "@types/merge-deep": "^3.0.0",
    "@types/node": "^14",
    "@types/etag": "^1.8.1",
    "@types/fresh": "^0.5.0",
    "@types/serve-static": "^1.13.10",
    "axios": "^1.2.1",
    "jest": "^27",
    "node-mocks-http": "^1.11.0",
    "portfinder": "^1.0.28",
    "typescript": "^4"
  },
  "sideEffects": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/types/index.d.ts"
  }
}
