{
  "name": "@modern-js/app-tools",
  "description": "A Progressive React Framework for modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/web-infra-dev/modern.js/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/modern.js",
    "directory": "packages/solutions/app-tools"
  },
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "3.0.0-alpha.1",
  "types": "./src/index.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm-node/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "node": {
        "import": "./dist/esm-node/index.mjs",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/cjs/index.js"
    },
    "./builder": {
      "types": "./dist/types/builder/index.d.ts",
      "node": {
        "import": "./dist/esm-node/builder/index.mjs",
        "require": "./dist/cjs/builder/index.js"
      },
      "default": "./dist/cjs/builder/index.js"
    },
    "./cli": {
      "types": "./dist/types/index.d.ts",
      "node": {
        "import": "./dist/esm-node/index.mjs",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/cjs/index.js"
    },
    "./cli/run": {
      "types": "./dist/types/run/index.d.ts",
      "node": {
        "import": "./dist/esm-node/run/index.mjs",
        "require": "./dist/cjs/run/index.js"
      },
      "default": "./dist/cjs/run/index.js"
    },
    "./types": {
      "types": "./lib/types.d.ts",
      "default": "./lib/types.d.ts"
    },
    "./server": {
      "types": "./dist/types/exports/server.d.ts",
      "node": {
        "import": "./dist/esm-node/exports/server.mjs",
        "require": "./dist/cjs/exports/server.js"
      },
      "default": "./dist/cjs/exports/server.js"
    }
  },
  "engines": {
    "node": ">=20"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "cli/run": [
        "./dist/types/run/index.d.ts"
      ],
      "types": [
        "./lib/types.d.ts"
      ],
      "server": [
        "./dist/types/exports/server.d.ts"
      ],
      "deploy": [
        "./dist/types/plugins/deploy/exports.d.ts"
      ],
      "builder": [
        "./dist/types/builder/index.d.ts"
      ]
    }
  },
  "bin": {
    "modern": "./bin/modern.js",
    "modern-app": "./bin/modern.js"
  },
  "scripts": {
    "prepublishOnly": "only-allow-pnpm",
    "dev": "rslib build --watch",
    "build": "rslib build",
    "test": "rstest"
  },
  "dependencies": {
    "@babel/parser": "^7.28.6",
    "@babel/traverse": "^7.28.6",
    "@babel/types": "^7.28.6",
    "@modern-js/builder": "workspace:*",
    "@modern-js/i18n-utils": "workspace:*",
    "@modern-js/plugin": "workspace:*",
    "@modern-js/plugin-data-loader": "workspace:*",
    "@modern-js/prod-server": "workspace:*",
    "@modern-js/server": "workspace:*",
    "@modern-js/server-core": "workspace:*",
    "@modern-js/server-utils": "workspace:*",
    "@modern-js/types": "workspace:*",
    "@modern-js/utils": "workspace:*",
    "@rsbuild/core": "2.0.0-alpha.4",
    "@swc/helpers": "^0.5.17",
    "es-module-lexer": "^1.7.0",
    "esbuild": "0.25.5",
    "esbuild-register": "^3.6.0",
    "flatted": "^3.3.3",
    "import-meta-resolve": "^4.2.0",
    "mlly": "^1.8.0",
    "ndepe": "^0.1.13",
    "pkg-types": "^1.3.1",
    "std-env": "^3.10.0"
  },
  "devDependencies": {
    "@modern-js/rslib": "workspace:*",
    "@rslib/core": "0.19.3",
    "@scripts/rstest-config": "workspace:*",
    "@types/babel__traverse": "7.28.0",
    "@types/node": "^20",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0"
  },
  "peerDependenciesMeta": {
    "ts-node": {
      "optional": true
    },
    "tsconfig-paths": {
      "optional": true
    }
  },
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/index.d.ts"
  }
}
