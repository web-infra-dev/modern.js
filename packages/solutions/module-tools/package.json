{
  "name": "@modern-js/module-tools",
  "description": "The meta-framework suite designed from scratch for frontend-focused modern web development.",
  "homepage": "https://modernjs.dev",
  "bugs": "https://github.com/modern-js-dev/modern.js/issues",
  "repository": "modern-js-dev/modern.js",
  "license": "MIT",
  "keywords": [
    "react",
    "framework",
    "modern",
    "modern.js"
  ],
  "version": "1.7.0",
  "bin": {
    "modern": "./bin/modern.js"
  },
  "jsnext:source": "./src/index.ts",
  "types": "./src/index.ts",
  "main": "./dist/js/node/index.js",
  "module": "./dist/js/treeshaking/index.js",
  "jsnext:modern": "./dist/js/modern/index.js",
  "exports": {
    ".": {
      "node": {
        "jsnext:source": "./src/index.ts",
        "import": "./dist/js/modern/index.js",
        "require": "./dist/js/node/index.js"
      },
      "default": "./dist/js/treeshaking/index.js"
    },
    "./cli": {
      "jsnext:source": "./src/index.ts",
      "default": "./dist/js/node/index.js"
    },
    "./build": {
      "jsnext:source": "./src/build.ts",
      "default": "./dist/js/node/build.js"
    }
  },
  "typesVersions": {
    "*": {
      "types": [
        "./lib/types.d.ts"
      ],
      "type": [
        "./lib/types.d.ts"
      ]
    }
  },
  "scripts": {
    "prepare": "pnpm build",
    "prepublishOnly": "only-allow-pnpm && pnpm build -- --platform",
    "new": "modern new",
    "build": "wireit",
    "test": "wireit",
    "dev": "modern build --watch"
  },
  "dependencies": {
    "@babel/generator": "^7.18.0",
    "@babel/parser": "^7.18.0",
    "@babel/runtime": "^7.18.0",
    "@babel/traverse": "^7.18.0",
    "@babel/types": "^7.18.0",
    "@modern-js/babel-compiler": "workspace:^1.2.6",
    "@modern-js/babel-preset-module": "workspace:^1.4.0",
    "@modern-js/core": "workspace:^1.13.0",
    "@modern-js/css-config": "workspace:^1.2.8",
    "@modern-js/new-action": "workspace:^1.3.12",
    "@modern-js/plugin": "workspace:^1.4.2",
    "@modern-js/plugin-changeset": "workspace:^1.4.0",
    "@modern-js/plugin-i18n": "workspace:^1.3.0",
    "@modern-js/plugin-jarvis": "workspace:^1.2.14",
    "@modern-js/style-compiler": "workspace:^1.2.13",
    "@modern-js/utils": "workspace:^1.7.12",
    "@rollup/plugin-json": "~4.1.0",
    "@speedy-js/speedy-types": "0.13.2-alpha.3",
    "@speedy-js/speedy-core": "0.13.2-alpha.3",
    "@speedy-js/speedy-plugin-es5": "0.13.2-alpha.3",
    "normalize-path": "^3.0.0",
    "p-map": "^4",
    "process.argv": "^0.6.0",
    "rollup": "^2.70.2",
    "rollup-plugin-dts": "^4.2.1",
    "rollup-plugin-hashbang": "^3.0.0",
    "signal-exit": "^3.0.7"
  },
  "devDependencies": {
    "@scripts/build": "workspace:*",
    "@scripts/jest-config": "workspace:*",
    "@speedy-js/speedy-types": "0.13.2-alpha.3",
    "@types/babel__core": "^7.1.15",
    "@types/babel__generator": "^7.6.3",
    "@types/babel__traverse": "^7.14.2",
    "@types/glob": "^7.1.4",
    "@types/jest": "^27",
    "@types/node": "^14",
    "@types/normalize-path": "^3.0.0",
    "@types/react": "^17",
    "@types/react-dom": "^17",
    "@types/signal-exit": "^3.0.1",
    "jest": "^27",
    "typescript": "^4",
    "ajv": "^8",
    "ajv-keywords": "^5"
  },
  "sideEffects": false,
  "modernConfig": {
    "output": {
      "packageMode": "node-js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "types": "./dist/types/index.d.ts"
  },
  "wireit": {
    "build": {
      "command": "modern build",
      "files": [
        "src/**/*",
        "tsconfig.json",
        "package.json",
        "./modern.config.js"
      ],
      "output": [
        "dist/**/*"
      ]
    },
    "test": {
      "command": "jest --passWithNoTests",
      "files": [
        "src/**/*",
        "tsconfig.json",
        "package.json",
        "tests/**/*"
      ],
      "output": []
    }
  }
}
