{
  "name": "@modern-js/storybook",
  "version": "2.54.5",
  "description": "modern.js support for storybook",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/modern.js",
    "directory": "packages/storybook/builder"
  },
  "jsnext:source": "./src/index.ts",
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "bin": {
    "storybook": "./bin.js",
    "sb": "./bin.js"
  },
  "files": [
    "dist",
    "bin.js"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch --incremental",
    "test": "vitest run"
  },
  "typesVersions": {
    "*": {
      "addons": [
        "./dist/addons.d.ts"
      ],
      "types": [
        "./dist/types.d.ts"
      ]
    }
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "jsnext:source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./addons": {
      "types": "./dist/addons.d.ts",
      "jsnext:source": "./src/addons.ts",
      "default": "./dist/addons.js"
    },
    "./preset": {
      "types": "./dist/preset.d.ts",
      "jsnext:source": "./src/preset.ts",
      "default": "./dist/preset.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "jsnext:source": "./src/types.ts",
      "default": "./dist/types.ks"
    },
    "./bin": "./bin.js",
    "./package.json": "./package.json"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "license": "MIT",
  "dependencies": {
    "@modern-js/storybook-builder": "workspace:*",
    "@storybook/react": "~7.6.1",
    "@modern-js/utils": "workspace:*",
    "storybook": "~7.6.1"
  },
  "devDependencies": {
    "@storybook/types": "~7.6.12",
    "typescript": "^5.2.2",
    "@types/node": "^20.5.6"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public",
    "provenance": true,
    "types": "./dist/index.d.ts"
  }
}
