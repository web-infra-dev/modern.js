{
  "name": "{{packageName}}",
  "version": "0.1.0",
  "scripts": {
    "reset": "npx rimraf node_modules ./**/node_modules",
    "dev": "modern dev",
    "build": "modern build",
    "serve": "modern serve"{{#unless isSubproject}},
    "lint": "biome check",
    "prepare": "simple-git-hooks"{{/unless}}
  },
  "engines": {
    "node": ">=20"
  }{{#unless isSubproject}},
  "lint-staged": {
    "*.{js,ts,cjs,mjs,d.cts,d.mts,jsx,tsx,json,jsonc}": [
      "biome check --files-ignore-unknown=true"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  }{{/unless}},
  "dependencies": {
    "@modern-js/runtime": "{{version}}"{{#if isTanstackRouter}},
    "@tanstack/react-router": "1.158.1"{{/if}},
    "react": "^19.2.3",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@modern-js/app-tools": "{{version}}",
    "@modern-js/tsconfig": "{{version}}"{{#if enableTailwind}},
    "@tailwindcss/postcss": "^4.1.18",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.18"{{/if}}{{#unless isSubproject}},
    "@biomejs/biome": "1.9.4"{{/unless}},
    "typescript": "~5.7.3",
    "@types/node": "^20",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6"{{#unless isSubproject}},
    "lint-staged": "~15.4.0",
    "simple-git-hooks": "^2.11.1"{{/unless}},
    "rimraf": "^6.0.1"
  }
}
