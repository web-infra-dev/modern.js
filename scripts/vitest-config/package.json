{
  "name": "@scripts/vitest-config",
  "private": true,
  "version": "1.20.1",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "tsc --watch",
    "build": "wireit",
    "prepare": "pnpm build",
    "test": "vitest run",
    "test:watch": "vitest dev --no-coverage"
  },
  "dependencies": {
    "@modern-js/tsconfig": "workspace:*",
    "@modern-js/utils": "workspace:*",
    "@types/node": "^14",
    "typescript": "^4.7.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "wireit": {
    "build": {
      "command": "tsc",
      "files": [
        "src/**/*",
        "tsconfig.json",
        "package.json"
      ],
      "output": [
        "dist/**/*"
      ]
    }
  }
}
