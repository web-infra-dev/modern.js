{
  "name": "app-docmuent",
  "version": "0.1.0",
  "scripts": {
    "bb": "cd ../../.. && pnpm --filter @modern-js/runtime build && pnpm --filter @modern-js/webpack build && pnpm --filter @modern-js/app-tools build && cd ./tests/integration/app-document && pnpm run dev",
    "reset": "npx rimraf ./**/node_modules",
    "dev": "modern dev",
    "build": "modern build",
    "start": "modern start",
    "new": "modern new",
    "lint": "modern lint"
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ],
    "*.{js,jsx,mjs,mjsx,cjs,cjsx}": [
      "node --max_old_space_size=8192 ./node_modules/eslint/bin/eslint.js --fix --color --cache --quiet"
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ],
  "dependencies": {
    "@modern-js/runtime": "workspace:*",
    "react": "^18",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@modern-js/app-tools": "workspace:*",
    "@modern-js/plugin-router-legacy": "workspace:*",
    "@modern-js/runtime": "workspace:*",
    "@modern-js/tsconfig": "workspace:*",
    "@types/node": "^14",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^4"
  }
}
