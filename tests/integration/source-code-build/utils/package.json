{
  "name": "@source-code-build/utils",
  "private": true,
  "version": "2.9.0",
  "main": "./dist/index.js",
  "source": "./src/index.ts",
  "types": "./dist/types/index.d.ts",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "common": [
        "./dist/types/common/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsc"
  },
  "devDependencies": {
    "typescript": "^5"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "default": "./dist/index.js"
    },
    "./common": {
      "source": "./src/common/index.ts",
      "default": "./dist/common/index.js"
    }
  },
  "engines": {
    "node": ">=14.17.6"
  },
  "eslintIgnore": [
    "node_modules/",
    "dist/"
  ]
}
